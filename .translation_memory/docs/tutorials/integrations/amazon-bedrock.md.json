    {
      "segment_id": "fa430ad4",
      "source_content": "## What is Amazon Bedrock",
      "source_content_hash": "c27b8214bd491506740ce8e2e9c2ad99d5d0e3f41bb029b781b54806af46af34",
      "node_type": "heading",
      "translatable": true,
