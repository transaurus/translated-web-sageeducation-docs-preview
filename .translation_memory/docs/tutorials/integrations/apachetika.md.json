      }
    },
    {
      "segment_id": "45022e4f",
      "source_content": "```bash\ndocker-compose up -d\n```",
      "source_content_hash": "c006e4c270559a4d23d6af53b381187a8754316cde7f04f1c995fff192d67ed1",
      "node_type": "code",
      "translatable": false,
      "translations": {
        "ja": "@@untranslatable_placeholder_45022e4f"
      }
